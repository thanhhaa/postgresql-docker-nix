# ============================================
# DEVELOPMENT ENVIRONMENT
# ============================================

# Container Names
DEV_POSTGRES_CONTAINER_NAME=your_postgres_container_here
DEV_PGADMIN_CONTAINER_NAME=your_pgadmin_container_here

# Database Configuration
DEV_POSTGRES_DB_NAME=your_postgres_database_here
DEV_POSTGRES_USER=your_postgres_user_here
DEV_POSTGRES_PASSWORD=your_postgres_password_here
DEV_POSTGRES_HOST=your_postgres_host_here
DEV_POSTGRES_PORT=your_postgres_port_here
DEV_POSTGRES_INTERNAL_PORT=your_postgres_internal_port_here

# pgAdmin Configuration
DEV_PGADMIN_DEFAULT_EMAIL=your_pgadmin_email_here
DEV_PGADMIN_DEFAULT_PASSWORD=your_pgadmin_password_here
DEV_PGADMIN_HOST=your_pgadmin_host_here
DEV_PGADMIN_PORT=your_pgadmin_port_here
DEV_PGADMIN_INTERNAL_PORT=your_pgadmin_internal_port_here

# Environment
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=debug

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_RETENTION_DAYS=30